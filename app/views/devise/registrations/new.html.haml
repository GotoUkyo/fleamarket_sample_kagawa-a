.AccountPage
  .AccountPage__title
    %h2 会員情報入力
  .AccountPage__form
    = form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f|
      = render "devise/shared/error_messages", resource: resource
      .FormField
        .FormField__label--normal
          = f.label :ニックネーム
        .FormField__input--normal
          = f.text_field :email,placeholder: "例）フリマ太郎", autofocus: true, autocomplete: "ニックネーム"
      .FormField
        .FormField__label--normal
          = f.label :メールアドレス
        .FormField__input--normal
          = f.email_field :email,placeholder: "PC・携帯どちらでも可", autofocus: true, autocomplete: "email"
      .FormField
        .FormField__label--normal
          = f.label :パスワード
          - if @minimum_password_length
            %em
              (#{@minimum_password_length} characters minimum)
        .FormField__input--normal
          = f.password_field :password,placeholder: "６文字以上の半角英数字",  autocomplete: "new-password"
      .FormField
        .FormField__label--normal
          = f.label :パスワード
        .FormField__input--normal
          = f.password_field :password_confirmation,placeholder: "上記と同じものを入力してください",  autocomplete: "new-password"
      %h3 本人確認
      %p 安心・安全にご利用いただくために、お客さまの本人
      %br情報の登録にご協力ください。他のお客様に公開さ
      %brれることはありません。
      .FormField
        .FormField__label--normal
          = f.label :お名前（全角）
        .FormField__input--name
          = f.text_field :email,placeholder: "例）香川", autofocus: true, autocomplete: "ニックネーム"
          = f.text_field :email,placeholder: "例）太郎", autofocus: true, autocomplete: "ニックネーム"
      .FormField
        .FormField__label--normal
          = f.label :お名前（カナ）
        .FormField__input--name
          = f.text_field :email,placeholder: "例）カガワ", autofocus: true, autocomplete: "ニックネーム"
          = f.text_field :email,placeholder: "例）タロウ", autofocus: true, autocomplete: "ニックネーム"
      .FormField
        .FormField__label--normal
          = f.label :生年月日
        .FormField__input--date
          = f.text_field :email, autofocus: true, autocomplete: "ニックネーム"
          = f.text_field :email, autofocus: true, autocomplete: "ニックネーム"
          = f.text_field :email, autofocus: true, autocomplete: "ニックネーム"
      %h3 配送先住所
      .FormField
        .FormField__label--normal
          = f.label :郵便番号
        .FormField__input--normal
          = f.text_field :email,placeholder: "例）760-0019", autofocus: true, autocomplete: "ニックネーム"
      .FormField
        .FormField__label--normal
          = f.label :都道府県
        .FormField__input--normal
          = f.text_field :email, autofocus: true, autocomplete: "ニックネーム"
      .FormField
        .FormField__label--normal
          = f.label :市区町村
        .FormField__input--normal
          = f.text_field :email,placeholder: "例）高松市", autofocus: true, autocomplete: "ニックネーム"
      .FormField
        .FormField__label--normal
          = f.label :番地
        .FormField__input--normal
          = f.text_field :email,placeholder: "例）サンポート２−１", autofocus: true, autocomplete: "ニックネーム"
      .FormField
        .FormField__label--normal
          = f.label :建物名
        .FormField__input--normal
          = f.text_field :email,placeholder: "例）高松シンボルタワー　タワー棟４・５階", autofocus: true, autocomplete: "ニックネーム"
      .FormField
        .FormField__label--normal
          = f.label :電話番号
        .FormField__input--normal
          = f.text_field :email,placeholder: "電話番号", autofocus: true, autocomplete: "ニックネーム"
        
      .actions
        = f.submit "登録する"
    = render "devise/shared/links"

